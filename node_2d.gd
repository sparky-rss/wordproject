func return_unique_array(input_array: Array) -> Array:
	var unique_values : Array = []
	var is_unique : bool = true
	for i in input_array:
		for j in unique_values:		
			if i == j:
				is_unique = false
				break
		if is_unique:
			unique_values.append(i)
		else:
			is_unique = true
	return unique_values

